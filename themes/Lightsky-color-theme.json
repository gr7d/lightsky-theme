  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#919191"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function.definition"],
      "settings": {
        "foreground": "#c3078e"
      }
    },
    {
      "name": "Variables, JSON keys, other keyword definitions",
      "scope": [
        "variable",
        "string.json.comments",
        "support.type.property-name.json",
        "keyword.other.definition"
      ],
      "settings": {
        "foreground": "#4b4ed0"
      }
    },
    {
      "name": "Object key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#a08415"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant"],
      "settings": {
        "foreground": "#cd1c05"
      }
    },
    {
      "name": "Classes, namespaces",
      "scope": ["entity.name.type.class", "entity.name.namespace"],
      "settings": {
        "foreground": "#c56900"
      }
    },
    {
      "name": "Types",
      "scope": ["meta.type", "entity.name.type", "support.type"],
      "settings": {
        "foreground": "#8046c2"
      }
    },
    {
      "name": "Strings, headings",
      "scope": ["string.quoted", "string.template", "string.regexp", "heading"],
      "settings": {
        "foreground": "#0c8f22"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute"],
      "settings": {
        "foreground": "#006a39"
      }
    },
    {
      "name": "Links",
      "scope": ["meta.link"],
      "settings": {
        "foreground": "#1c1caa"
      }
    },
    {
      "name": "Markup code/inline",
      "scope": ["markup.fenced_code", "markup.inline"],
      "settings": {
        "foreground": "#111"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8d0202"
      }
    },
    {
      "name": "Tag attributes",
      "scope": ["meta.tag.attributes"],
      "settings": {
        "foreground": "#b22b3b"
      }
    },
    {
      "name": "Storage types",
      "scope": ["storage"],
      "settings": {
        "foreground": "#127f92"
      }
    },
    {
      "name": "Keyword control, other keywords",
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#b130ce"
      }
    },
    {
      "name": "Keyword op",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#6d1f7f"
      }
    },
    {
      "name": "text",
      "scope": ["text"],
      "settings": {
        "foreground": "#555"
      }
    }
  ]
}
